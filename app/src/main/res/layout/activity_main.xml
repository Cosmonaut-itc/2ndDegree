<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <EditText
        android:id="@+id/addCText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Add c"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="73dp"
        tools:layout_editor_absoluteY="300dp" />

    <EditText
        android:id="@+id/addBtext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Add b"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="73dp"
        tools:layout_editor_absoluteY="240dp" />

    <TextView
        android:id="@+id/bTextView"
        android:layout_width="18dp"
        android:layout_height="36dp"
        android:text="b"
        android:textAlignment="center"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="249dp" />

    <TextView
        android:id="@+id/cTextView"
        android:layout_width="18dp"
        android:layout_height="36dp"
        android:text="c"
        android:textAlignment="center"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="309dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="381dp"
        android:layout_height="95dp"
        app:srcCompat="@drawable/ecuaciones_de_segundo_grado_como_resolverlas"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="33dp" />

    <TextView
        android:id="@+id/aTextView"
        android:layout_width="18dp"
        android:layout_height="36dp"
        android:text="a"
        android:textAlignment="center"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="194dp" />

    <EditText
        android:id="@+id/addAText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Add a"
        tools:layout_editor_absoluteX="73dp"
        tools:layout_editor_absoluteY="185dp" />
</androidx.constraintlayout.widget.ConstraintLayout>